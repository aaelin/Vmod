    transform run:
        anchor (0.0, 0.0) pos (0.0, 0.0)
        linear 0.1 pos (-9, -7)
        linear 0.1 pos (0, 0)
        linear 0.1 pos (9, -7)
        linear 0.1 pos (0, 0)
        repeat
    transform dance:
        anchor (0.0, 0.0) pos (0.0, 0.0)
        linear 0.5 pos (-12, -9)
        linear 0.5 pos (0, 0)
        linear 0.5 pos (12, -9)
        linear 0.5 pos (0, 0)
        repeat
    transform slow_dance:
        anchor (0.0, 0.0) pos (0.0, 0.0)
        linear 0.7 pos (-12, -9)
        linear 0.7 pos (0, 0)
        linear 0.7 pos (12, -9)
        linear 0.7 pos (0, 0)
        repeat
    transform train_rumble:
        # Стоит учесть, что рассчитано для изображений размером 2400х1350
        # Работает корректно для этого размера, 2 первых параметра изменяй в зависимости от размера png
        xoffset 500 yoffset 50
        anchor (0.35, 0.35)  # Центр изображения
        linear 2.0  pos (-8, 0) rotate -0.3 # Смещение вправо, небольшой наклон влево
        linear 2.0  pos (8, 0) rotate 0.3  # Смещение влево, небольшой наклон вправо
        # Качаем вверх-вниз
        linear 1  pos (-2, -5) rotate 0 # Смещение вниз, выравниваем
        linear 1 pos (2, 5) rotate 0 # Смещение вниз, выравниваем
        repeat
