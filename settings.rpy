    def register_image_folder(folder_path, base_path="mods/Vmod/"):
        trim_length = len(base_path + folder_path)
        for filename in renpy.list_files():
            if filename.startswith(base_path + folder_path) and filename.endswith((".jpg", ".png")):
                image_name = filename[trim_length:-4]
                renpy.image(image_name, filename) # Регистрируем изображение
